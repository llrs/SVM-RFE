citHeader("To cite mRFE in publications use:")

bibentry(bibtype = "Article",
    title = "Gene selection for cancer classification using support vector machines",
    volume = "46",
    journal = "Machine learning",
    author = personList(as.person("Guyon, Isabelle"),
                        as.person("Weston, Jason"),
                        as.person("Barnhill, Stephen"),
                        as.person("Vapnik, Vladimir")),
    year = "2002",
    note = "Original SVM-RFE paper.",
    pages = "389-422"
)


